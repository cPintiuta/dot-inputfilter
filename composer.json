{
  "name": "dotkernel/dot-inputfilter",
  "description": "DotKernel input filter component extending and customizing zend-inputfilter",
  "authors": [
    {
      "name": "DotKernel Team",
      "email": "team@dotkernel.com"
    }
  ],
  "type": "library",
  "license": "MIT",
  "keywords": [
    "dotkernel",
    "input filter",
    "factories",
    "container",
    "zf2",
    "zf3",
    "service-manager"
  ],
  "require": {
    "php": "^7.1",
    "zendframework/zend-servicemanager": "^3.3",
    "zendframework/zend-inputfilter": "^2.8",

    "dotkernel/dot-filter": "^1.1",
    "dotkernel/dot-validator": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.7",
    "squizlabs/php_codesniffer": "^2.5"
  },
  "autoload": {
    "psr-4": {
      "Dot\\InputFilter\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DotTest\\InputFilter\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1-dev",
      "dev-develop": "1.2-dev"
    }
  }
}
